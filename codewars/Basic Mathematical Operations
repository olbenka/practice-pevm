
int basicOp(char op, int val1, int val2) {
  // Your code here
  int res = 0;
  if (op=='+'){
    res += val1+val2;
  }else{
    if (op=='-'){
      res += val1-val2;
    } else {
      if (op == '*'){
        res += val1*val2;
      } else {
        res += (int)val1/val2;
      }
    }
  }
  return res;
}
