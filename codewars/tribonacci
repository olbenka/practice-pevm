std::vector<int> tribonacci(std::vector<int> signature, int n)
{
    if (n==0){
      std::vector<int> s1;
      s1.reserve(n); 
      return s1;
    } 
    else {
      std::vector<int> s(n);
      s [0] += signature [0];
      s [1] += signature [1];
      s [2] += signature [2];
      
      for (int i = 3; i < n; ++i){
        s[i] += s[i-3] + s[i-2] + s[i-1];
      }
    return s;
    }
}
